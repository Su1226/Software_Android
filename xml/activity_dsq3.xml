<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DSQ3Activity">

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "8"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "거스름돈을 계산하거나, 돈을 정확히 세서 지불하는 것은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "9"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "이야기 도중에 머뭇거리거나 말문이 막히는 것은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "10"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "이야기 도중에 물건의 이름을 정확히 대는 정도는 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "11"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "가까운 사람의 이름을 기억하는 것은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "12"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "가까운 사람에 관한 사항, 즉 사는 곳이나 직업 등을 기억하는 것은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "13"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "며자신의 주소나 전화번호를 기억하는 것은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "14"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "전화, 가스레인지, 텔레비전 등 집안에서 늘 사용하던 가구를 다루는 능력은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "15"
            android:layout_margin="10dp"
            android:textSize = "15dp">
        </TextView>

        <TextView
            android:layout_width = "wrap_content"
            android:layout_height="wrap_content"
            android:text = "어떤 옷을 입고 나갈지, 저녁식사에 무엇을 준비할지 등 일상적인 상황에서 결정을 내리는 능력은 어떻습니까?"
            android:layout_margin="10dp"
            android:textSize = "15dp"
            android:layout_weight="1">
        </TextView>
    </TableRow>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text= "결과보기"
        android:id="@+id/K2_btn1">
    </Button>

</TableLayout>