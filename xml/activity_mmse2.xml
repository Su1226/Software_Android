        android:text = "시작하기"
        android:id = "@+id/MM_btn1">
    </Button>


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text = "끝내기"
        android:id = "@+id/MM_btn2">
